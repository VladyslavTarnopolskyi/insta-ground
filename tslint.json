{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "eofline": false,
    "no-trailing-whitespace": false,
    "comment-format": false,
    "no-console": false,
    "one-line": false,
    "no-consecutive-blank-lines": false,
    "curly": true,
    "no-shadowed-variable": false,
    "array-type": [
      true,
      "array"
    ],
    "quotemark": [
      false,
      "single",
      "double",
      "avoid-escape"
    ],
    "semicolon": [
      true,
      "always"
    ]
  },
  "rulesDirectory": []
}
